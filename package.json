{
  "name": "svelte-pivottable",
  "version": "0.2.1",
  "description": "A Svelte-based pivot table with grouping forked from \"https://github.com/jjagielka/svelte-pivottable#readme\"",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
