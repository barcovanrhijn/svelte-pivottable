    "url": "git+https://github.com/barovanrhijn/svelte-pivottable.git"
  },
  "keywords": [
    "svelte",
    "sevltekit",
    "pivottable"
  ],
  "author": {
    "name": "Barco van Rhijn",
    "email": "barcovanrhijn@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/barcovanrhijn/svelte-pivottable/issues"
  },
  "homepage": "https://github.com/barcovanrhijn/svelte-pivottable#readme",
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.6",
    "@sveltejs/kit": "^1.5.5",
    "@sveltejs/package": "^1.0.2",
    "svelte-check": "^3.0.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.1"
  },
  "type": "module",
